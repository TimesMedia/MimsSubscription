@using Subs.Data

@model ProductDoc.ProductDataTable

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ProductCatalog</title>
</head>
<body>
    <table>
        <thead><tr><th>Product</th></tr></thead>
        <tbody>
            @foreach (ProductDoc.ProductRow p in Model)
            {
                <tr>
                    <td>@p.ProductName</td>
                </tr>
            }
        </tbody>
    </table>

</body>
</html>
